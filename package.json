{
  "name": "mili",
  "version": "3.8.0",
  "description": "Scaffolding with continuous control over the development of the project.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Val-istar-Guo/mili.git"
  },
  "bugs": {
    "url": "https://github.com/Val-istar-Guo/mili/issues"
  },
  "homepage": "https://github.com/Val-istar-Guo/mili#readme",
  "keywords": [
    "mili",
    "scaffold",
    "template",
    "templates",
    "continuous"
  ],
  "author": "Val.istar.Guo <val.istar.guo@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.4"
  },
  "bin": {
    "mili": "./bin/mili"
  },
  "scripts": {
    "clean": "rm -rf ./lib/*",
    "ca": "git add -A && git-cz -av",
    "ci": "git-cz",
    "release": "standard-version --no-verify",
    "prepublishOnly": "npm test && npm run build",
    "postpublish": "git push",
    "test": "ava",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "prereport": "rm -rf .nyc_output coverage",
    "report": "nyc --reporter=html --reporter=text-summary --skip-full ava",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"  --fix",
    "prebuild": "npm run clean",
    "build:es": "ttsc -p build/tsconfig.es.json",
    "build:lib": "ttsc -p build/tsconfig.lib.json",
    "build": "npm run build:lib && npm run build:es",
    "watch": "NODE_ENV=development ttsc -p build/tsconfig.lib.json -w",
    "test:update": "ava -u",
    "test:debug": "ava --verbose",
    "release:alpha": "standard-version --prerelease alpha",
    "release:first": "standard-version --first-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "ava": "^3.2.0",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.9",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "nyc": "^15.0.0",
    "sinon": "^8.1.1",
    "standard-version": "^7.1.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.7.5",
    "typescript-transform-paths": "^1.1.14"
  },
  "dependencies": {
    "@types/cosmiconfig": "^6.0.0",
    "@types/diff": "^4.0.2",
    "@types/fs-extra": "^8.0.1",
    "@types/js-yaml": "^3.12.2",
    "@types/micromatch": "^4.0.1",
    "@types/node": "^13.7.0",
    "@types/ramda": "^0.26.40",
    "@types/semver": "^7.1.0",
    "ajv": "^6.11.0",
    "ajv-keywords": "^3.4.1",
    "chalk": "^3.0.0",
    "commander": "^4.1.1",
    "cosmiconfig": "^6.0.0",
    "diff": "^4.0.2",
    "ejs": "^3.0.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.4",
    "js-yaml": "^3.13.1",
    "log4js": "^6.1.1",
    "merge-deep": "^3.0.2",
    "micromatch": "^4.0.2",
    "mustache": "^4.0.0",
    "ramda": "^0.27.0",
    "semver": "^7.1.2",
    "simple-git": "^1.131.0",
    "ts-custom-error": "^3.1.1",
    "validate-npm-package-name": "^3.0.0"
  }
}
