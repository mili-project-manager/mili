{
  "name": "mili",
  "version": "3.7.0",
  "description": "Scaffolding with continuous control over the development of the project.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Val-istar-Guo/mili.git"
  },
  "bugs": {
    "url": "https://github.com/Val-istar-Guo/mili/issues"
  },
  "homepage": "https://github.com/Val-istar-Guo/mili#readme",
  "keywords": [
    "mili",
    "scaffold",
    "template",
    "templates",
    "continuous"
  ],
  "author": "Val.istar.Guo <val.istar.guo@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.4"
  },
  "bin": {
    "mili": "./bin/mili"
  },
  "scripts": {
    "clean": "rm -rf ./lib/*",
    "ca": "git add -A && git-cz -av",
    "ci": "git-cz",
    "release": "standard-version --no-verify",
    "prepublishOnly": "npm test && npm run build",
    "postpublish": "git push",
    "test": "ava",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "prereport": "rm -rf .nyc_output coverage",
    "report": "nyc --reporter=html --reporter=text-summary --skip-full ava",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"  --fix",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "watch": "NODE_ENV=development rollup -c -w",
    "test:update": "ava -u",
    "test:debug": "ava --verbose",
    "release:alpha": "standard-version --prerelease alpha",
    "release:first": "standard-version --first-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "ava": "^2.1.0",
    "commitizen": "^3.1.1",
    "coveralls": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.0.1",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "nyc": "^14.1.1",
    "rollup": "^1.16.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-ts-paths": "^1.0.3",
    "rollup-plugin-typescript": "^1.0.1",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.12",
    "standard-version": "^6.0.1",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "dependencies": {
    "@types/cosmiconfig": "^5.0.3",
    "@types/diff": "^4.0.2",
    "@types/fs-extra": "^8.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/micromatch": "^3.1.0",
    "@types/node": "^12.0.10",
    "@types/ramda": "^0.26.12",
    "@types/semver": "^6.0.1",
    "ajv": "^6.10.0",
    "ajv-keywords": "^3.4.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "cosmiconfig": "^5.2.1",
    "diff": "^4.0.1",
    "ejs": "^2.6.2",
    "fs-extra": "^8.1.0",
    "inquirer": "^6.4.1",
    "js-yaml": "^3.13.1",
    "log4js": "^4.4.0",
    "merge-deep": "^3.0.2",
    "micromatch": "^4.0.2",
    "mustache": "^3.0.1",
    "ramda": "^0.26.1",
    "semver": "^6.1.2",
    "simple-git": "^1.116.0",
    "ts-custom-error": "^3.1.0",
    "validate-npm-package-name": "^3.0.0"
  }
}
